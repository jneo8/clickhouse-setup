<yandex>
    <clickhouse_remote_servers>
        <storage>
            <shard>
                <internal_replication>true</internal_replication>
                <replica>
                    <host>storage-01</host>
                    <port>9000</port>
                </replica>
                <replica>
                    <host>storage-02</host>
                    <port>9000</port>
                </replica>
                <replica>
                    <host>storage-03</host>
                    <port>9000</port>
                </replica>
            </shard>
        </storage>
        <query>
            <shard>
                <internal_replication>true</internal_replication>
                <replica>
                    <host>query-01</host>
                    <port>9000</port>
                </replica>
                <replica>
                    <host>query-02</host>
                    <port>9000</port>
                </replica>
            </shard>
        </query>
    </clickhouse_remote_servers>
    <networks>
        <ip>::/0</ip>
    </networks>
    <clickhouse_compression>
        <case>
            <min_part_size>10000000000</min_part_size>
            <min_part_size_ratio>0.01</min_part_size_ratio>
            <method>lz4</method>
        </case>
    </clickhouse_compression>
</yandex>
